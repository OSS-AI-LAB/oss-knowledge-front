# .env.example
# 이 파일을 .env로 복사하여 사용하세요

# API 설정
VITE_API_BASE_URL=http://localhost:3000/api
VITE_API_KEY=your_api_key_here

# WebSocket 설정 (옵션)
VITE_WS_URL=ws://localhost:3000

# 기능 플래그
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_VOICE_INPUT=false
VITE_ENABLE_CODE_EXECUTION=false

# 분석 도구 (옵션)
VITE_GA_ID=
VITE_SENTRY_DSN=

# 개발 설정
VITE_DEV_MODE=true
VITE_MOCK_API=true